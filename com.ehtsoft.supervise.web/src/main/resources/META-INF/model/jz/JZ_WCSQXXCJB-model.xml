<?xml version="1.0" encoding="UTF-8"?>

<model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../schema.xsd">
  <table name="JZ_WCSQXXCJB" label="外出申请信息采集表">
    <column field="ID" type="uuid" length="36" label="主键" required="true" remark="系统自动生成"/>
    <column field="F_AID" type="uuid" length="36" label="社区矫正人员id"/>
    <column field="SQJZRYBH" type="string" length="20" label="社区矫正人员编号" remark="编码见 9.7"/>
    <column field="SQBT" type="string" length="50" label="申请标题"/>
    <column field="SQSJ" type="timestamp" label="申请时间" remark="自填，格式按照 GB/T7408 中规定"/>
    <column field="WCMDDSZS" type="string" length="20" label="外出目的地所在省（区、市）" remark="SYS092"/>
    <column field="WCMDDSZD" type="string" length="20" label="外出目的地所在地（市、州）" remark="SYS092"/>
    <column field="WCMDDSZX" type="string" length="20" label="外出目的地所在县（市、区）" remark="SYS092"/>
    <column field="WCMDDXZ" type="string" length="20" label="外出目的地（乡镇、街道）" remark="自填"/>
    <column field="WCMDDMX" type="string" length="50" label="外出目的地明细" remark="自填"/>
    <column field="WCLY" type="string" length="500" label="外出理由" remark="自填"/>
    <column field="WCTS" type="integer" label="外出天数" remark="自填"/>
    <column field="KSQR" type="timestamp" label="外出开始时间" remark="自填，格式按照 GB/T7408 中规定"/>
    <column field="JSRQ" type="timestamp" label="外出结束时间" remark="自填，格式按照 GB/T7408 中规定"/>
    <column field="SFSSHR" type="string" length="20" label="司法所审核人" remark="自填"/>
    <column field="SFSSHSJ" type="timestamp" label="司法所审核时间" remark="自填，格式按照 GB/T7408 中规定"/>
    <column field="SFSSHYJ" type="string" length="500" label="司法所审核意见" remark="自填"/>
    <column field="SFSSHJG" type="string" length="20" label="司法所审核结果" remark="自填"/>   
    <column field="XSFJSPR" type="string" length="20" label="县（市、区）司法局审批人" remark="自填"/>
    <column field="XSFJSPSJ" type="timestamp" label="县（市、区）司法局审批时间" remark="自填，格式按照 GB/T7408 中规定"/>
    <column field="XSFJSPYJ" type="string" length="500" label="县（市、区）司法局审批意见" remark="自填"/>
    <column field="XSFJSPJG" type="string" length="20" label="县（市、区）司法局审批结果" remark="自填"/>
    <column field="JTGJ" type="string" length="30" label="交通工具"/>
    <column field="YJCFSJ" type="string" length="20" label="预计出发时间" remark="yyyy-MM-dd HH:mm"/>
    <column field="XJRQ" type="string" length="20" label="销假日期" remark="yyyy-MM-dd HH:mm"/>
    <column field="audit" type="integer" length="1" defaultValue="0" label="审核标记" remark="0、未审核状态 1、通过"/>
    <column field="cts" type="timestamp" label="创建时间戳" remark="数据创建（插入）保存当前插入数据的毫秒数"/>
    <column field="uts" type="timestamp" label="更新时间戳" remark="数据更新操作时，更新其属性为当前的毫秒数"/>
    <column field="cdate" type="integer" length="8" label="创建日期" remark="创建日期(不含时分秒) 数据格式 yyyyMMddd 数字类型，如：20150202"/>
    <column field="udate" type="integer" length="8" label="更新日期" remark="修改日期(不含时分秒) 数据格式 yyyyMMddd 数字类型，如：20150202"/>
    <column field="cuid" type="uuid" length="36" label="当前创建人员" remark="当前创建的人员id :user.getUid() (core_personnel.sysid)"/>
    <column field="uuid" type="uuid" length="36" label="当前更新人员" remark="当前更新的人员id :user.getUid() (core_personnel.sysid)"/>
    <column field="caid" type="uuid" length="36" label="当前创建账号" remark="当前创建的账号ID :user.getAid() (core_account.sysid)"/>
    <column field="uaid" type="uuid" length="36" label="当前更新账号" remark="当前更新的账号ID :user.getAid() (core_account.sysid)"/>
    <column field="orgid" type="uuid" length="36" label="当前机构" remark="当前登录人机构ID: user.getOrgid()(core_organization.sysid)"/>
    <column field="remark" type="string" length="500" label="备注"/>
    <primary name="PK_JZ_WCSQXXCJB" field="ID"/>
    <foreign name="FK_JZ_WCSQXXCJB_AID" field="F_AID" reference="JZ_JZRYJBXX" referField="ID"/>
  </table>
</model>

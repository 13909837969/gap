<?xml version="1.0" encoding="UTF-8"?>

<model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../schema.xsd">
  <table name="JZ_JZXZCY" label="矫正小组成员信息采集表">
    <column field="XZID" type="uuid" length="36" label="主键" required="true" remark="系统自动生成"/>
    <column field="F_AID" type="uuid" length="36" label="社区矫正人员ID"/>
    <column field="SQJZRYBH" type="string" length="20" label="社区矫正人员编号" remark="从社区矫正基本信息来的"/>
    <column field="XZCYLX" type="string" length="4" label="小组成员类型" remark="数据字典：SYS020"/>
    <column field="XZCYLB" type="string" length="4" label="小组成员类别" remark="数据字典：SYS021"/>
    <column field="XM" type="string" length="20" label="姓名"/>
    <column field="XB" type="string" length="2" label="性别" remark="数据字典：SYS000"/>
    <column field="CSRQ" type="string" length="20" label="出生日期" remark="格式:yyyy-MM-dd"/>
    <column field="SFZH" type="string" length="20" label="身份证号"/>
    <column field="XL" type="string" length="4" label="学历" remark="数据字典：SYS093"/>
    <column field="ZGXW" type="string" length="4" label="最高学位" remark="数据字典：SYS094"/>
    <column field="ZZMM" type="string" length="4" label="政治面貌" remark="数据字典：SYS091"/>
    <column field="ZY" type="string" length="8" label="专业" remark="数据字典：SYS095"/>
    <column field="ZHY" type="string" length="20" label="职业" remark="数据字典：SYS090"/>
    <column field="GZDW" type="string" length="100" label="工作单位"/>
    <column field="LXDH" type="string" length="20" label="联系电话"/>
    <column field="SJ" type="string" length="20" label="手机"/>
    <column field="JTZZ" type="string" length="100" label="家庭住址"/>
    <column field="SHGZZYLZC" type="string" length="4" label="社会工作专业类职称" remark="数据字典：SYS026"/>
    <column field="cts" type="timestamp" label="创建时间戳" remark="数据创建（插入）保存当前插入数据的毫秒数"/>
    <column field="uts" type="timestamp" label="更新时间戳" remark="数据更新操作时，更新其属性为当前的毫秒数"/>
    <column field="cdate" type="integer" length="8" label="创建日期" remark="创建日期(不含时分秒) 数据格式 yyyyMMddd 数字类型，如：20150202"/>
    <column field="udate" type="integer" length="8" label="更新日期" remark="修改日期(不含时分秒) 数据格式 yyyyMMddd 数字类型，如：20150202"/>
    <column field="cuid" type="uuid" length="36" label="当前创建人员" remark="当前创建的人员id :user.getUid() (core_personnel.sysid)"/>
    <column field="uuid" type="uuid" length="36" label="当前更新人员" remark="当前更新的人员id :user.getUid() (core_personnel.sysid)"/>
    <column field="caid" type="uuid" length="36" label="当前创建账号" remark="当前创建的账号ID :user.getAid() (core_account.sysid)"/>
    <column field="uaid" type="uuid" length="36" label="当前更新账号" remark="当前更新的账号ID :user.getAid() (core_account.sysid)"/>
    <column field="orgid" type="uuid" length="36" label="当前机构" remark="当前登录人机构ID: user.getOrgid()(core_organization.sysid)"/>
    <column field="remark" type="string" length="500" label="备注"/>
    <primary name="PK_JZ_JZXZCY" field="XZID"/>
  </table>
</model>

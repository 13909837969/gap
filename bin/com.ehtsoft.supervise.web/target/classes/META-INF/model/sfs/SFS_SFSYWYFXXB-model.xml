<?xml version="1.0" encoding="UTF-8"?>

<model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../schema.xsd">
  <table name="SFS_SFSYWYFXXB" label="司法所业务用房信息表">
    <column field="ID" type="uuid" length="36" label="主键" required="true" remark="系统自动生成"/>
    <column field="S_ID" type="string" length="36" label="司法所ID"  />
    <column field="SFSBM" type="string" length="36" label="司法所编码" />
    <column field="DLYWYF" type="string" length="10" label="独立业务用房"/>
    <column field="BGCSSJ" type="string" length="10" label="办公场所实景"/>
    <column field="YWYFCQGS" type="string" length="10"  label="业务用房产权归属"/>
    <column field="YWYFTZF" type="string" length="10" label="业务用房投资方"/>
    <column field="YWYFXZ" type="string" length="10" label="业务用房性质"/>
    <column field="YWYFMJ" type="double" length="10" label="业务用房面积"/>
    <column field="YWYFTZJE" type="double" length="10" label="业务用房投资金额"/>
    <column field="JCSJ" type="timestamp" label="建成时间"/>
    <column field="cts" type="timestamp" label="创建时间戳" remark="数据创建（插入）保存当前插入数据的毫秒数"/>

<?xml version="1.0" encoding="UTF-8"?>

<model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../schema.xsd">
  <table name="RMTJ_JJBJ_MEDIA" label="调解笔录图片及视频数据">
    <column field="IMGID" type="uuid" length="36" label="主键" required="true" remark="矫正人员ID"/>
    <column field="data" type="binary" label="头像数据"/>
    <column field="mimetype" type="string" length="20" label="图像类型" remark="图片类型如：image/png"/>
    <column field="JJBJID" type="uuid" length="36" label="调解笔记ID" remark="JJBJID = RMTJ_TJAJXX.ID"/>
    <column field="remark" type="string" length="500" label="备注"/>
    <primary name="PK_RMTJ_JJBJ_MEDIA" field="IMGID"/>
  </table>
</model>

<?xml version="1.0" encoding="UTF-8"?>

<model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../schema.xsd">
  <table name="JZ_PXQKDJB" label="培训情况登记表">
    <column field="PXQKID" type="uuid" length="36" label="主键" required="true" remark="系统自动生成"/>
    <column field="PXZTMC" type="string" length="50" label="培训主题名称"/>
    <column field="YCJRS" type="integer" length="3" label="应参加人数"/>
    <column field="SJCJRS" type="integer" length="3" label="实际参加人数"/>
    <column field="PXLSMC" type="string" length="30" label="培训老师名称"/>
    <column field="PXKSJ" type="string" length="16" label="培训开始时间" remark="2017-11-24 16:27"/>
    <column field="PXJSSJ" type="string" length="16" label="培训结束时间" remark="2017-11-24 16:27"/>
    <column field="PXDZ" type="string" length="200" label="培训地址"/>
    <column field="PXNRMS" type="string" length="200" label="培训内容描述"/>
    <column field="KSQKFX" type="string" length="200" label="考试情况分析"/>
    <column field="PXJGPJ" type="string" length="200" label="培训结果平价"/>
    <column field="del" type="integer" length="1" defaultValue="Sun Dec 31 00:00:00 CST 1899" label="删除标记" remark="0 没有删除   1 已经被删除"/>
    <column field="audit" type="integer" length="1" defaultValue="Sun Dec 31 00:00:00 CST 1899" label="审核标记" remark="0、未审核状态 1、通过"/>
    <column field="cts" type="timestamp" label="创建时间戳" remark="数据创建（插入）保存当前插入数据的毫秒数"/>
    <column field="uts" type="timestamp" label="更新时间戳" remark="数据更新操作时，更新其属性为当前的毫秒数"/>
    <column field="cdate" type="integer" length="8" label="创建日期" remark="创建日期(不含时分秒) 数据格式 yyyyMMddd 数字类型，如：20150202"/>
    <column field="udate" type="integer" length="8" label="更新日期" remark="修改日期(不含时分秒) 数据格式 yyyyMMddd 数字类型，如：20150202"/>
    <column field="cuid" type="uuid" length="36" label="当前创建人员" remark="当前创建的人员id :user.getUid() (core_personnel.sysid)"/>
    <column field="uuid" type="uuid" length="36" label="当前更新人员" remark="当前更新的人员id :user.getUid() (core_personnel.sysid)"/>
    <column field="caid" type="uuid" length="36" label="当前创建账号" remark="当前创建的账号ID :user.getAid() (core_account.sysid)"/>
    <column field="uaid" type="uuid" length="36" label="当前更新账号" remark="当前更新的账号ID :user.getAid() (core_account.sysid)"/>
    <column field="orgid" type="uuid" length="36" label="当前机构" remark="当前登录人机构ID: user.getOrgid()(core_organization.sysid)"/>
    <column field="remark" type="string" length="500" label="备注"/>
    <primary name="PK_JZ_PXQKDJB" field="PXQKID"/>
  </table>
</model>

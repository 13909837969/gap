			   	   	}else if(7<c.getFullYear()-1970<=10){
			   	   		$("#yqtxqx input").val('02');
			   	   	}else if(10<c.getFullYear()-1970<=25){
			   	   		$("#yqtxqx input").val('01');
			   	   	}

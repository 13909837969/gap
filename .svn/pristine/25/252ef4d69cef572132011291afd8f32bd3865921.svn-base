		<script type="text/javascript">
		//$("#alert-panel").alert();
		$(function() {
			
			var form = new Eht.Form({
				selector : "#form",
				autolayout : true,
				formCol:1
			});
			
			$("#button").click(function(){
				console.log(form.getData());
				form.clearValidStyle();
				form.validate();
			});
			
			var form1 = new Eht.Form({selector:"#form1",autolayout:false,formCol:2});
			form1.codeCombo(function(name,thisObj,codeData){
				if(name=="xb"){
					var div = $("<div></div>");
					for(var i=0;i<codeData.length;i++){
						div.append("<input name='"+name+"' type='radio' value='"+codeData[i].f_value+"'/>" + codeData[i].f_name);
					}
					console.log(thisObj);
					thisObj.replaceWith(div);
				}
			});
			form1.change(function(name,value){
				if(name=="sfwd"){
					if(value=="00"){
						this.hideCombo("sfws");
						//this.disable("sfws");
					}else{
						this.showCombo("sfws");
						//this.enable("sfws");
					}
				}
			});
			form.blur(function(name,value,obj){
				obj.disable();
			});
			form1.init();
			
			$("#savetest").click(function() {
				form1.setValue("sfwd","00",true);
				//form1.fill({"sfwd":"00"},true);
				if (form1.validate()) {
					//向后台保存数据  service.insert();
				}
			});
			
			$("#alertbtn").click(function(){
				new Eht.Alert().show("");
			});
		});
	</script>

